#parse("/velocity/layout.vm")
#@mainLayout()

    <section class="engine">
        <a rel="external" href="https://mobirise.com">Mobirise Web Creator</a></section><section class="mbr-section mbr-section-md-padding mbr-footer footer2 mbr-after-navbar" id="contacts2-a" style="background-color: rgb(46, 46, 46); padding-top: 90px; padding-bottom: 90px;">
        <h2>Reports</h2>
        <br/>
        <fieldset class="report-field">
          <legend class="report-border">System</legend>
          <p>Uptime  : $SystemUpTime<p>
          <p>Memory  : $UsedMemory MByte</p>
          <p>Version : $SystemName</p>
        </fieldset>  
        <fieldset class="report-field">
          <legend class="report-border">Bot usage</legend>
            <p>Bot conversations : $Botrequestcounter<p>
            <p>Translation requests : $Translatencounter<p>    
            <p>Maja currently invited in $Majarooms rooms</p>
            <p>BankDemo currently invited in $Bankdemorooms rooms</p>
         </fieldset>
        <fieldset class="report-field">
          <legend class="report-border">Webserver usage</legend>
            <p>Sessions : $Webrequests<p>
         </fieldset>        
    </section>

#end